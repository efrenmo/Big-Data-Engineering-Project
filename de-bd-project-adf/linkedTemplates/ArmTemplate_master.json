{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "de-bd-project-adf"
		},
		"AmazonS31_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'AmazonS31'"
		},
		"ls_ablob_dephase2projectdsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_dephase2projectdsa'"
		},
		"ls_ablob_wcd_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_wcd'"
		},
		"ls_pg_rds_wcd_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_pg_rds_wcd'"
		},
		"AmazonS31_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIAV4NG4RPRG2356OOP"
		},
		"ls_adls_ingestiondl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://debdingestiondl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "de-bd-project-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AmazonS31_secretAccessKey": {
						"value": "[parameters('AmazonS31_secretAccessKey')]"
					},
					"ls_ablob_dephase2projectdsa_connectionString": {
						"value": "[parameters('ls_ablob_dephase2projectdsa_connectionString')]"
					},
					"ls_ablob_wcd_connectionString": {
						"value": "[parameters('ls_ablob_wcd_connectionString')]"
					},
					"ls_pg_rds_wcd_connectionString": {
						"value": "[parameters('ls_pg_rds_wcd_connectionString')]"
					},
					"AmazonS31_properties_typeProperties_accessKeyId": {
						"value": "[parameters('AmazonS31_properties_typeProperties_accessKeyId')]"
					},
					"ls_adls_ingestiondl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_ingestiondl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}